{
    "name": "invoice-number",
    "version": "1.0.6",
    "description": "Generate invoice number sequentially",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "test": "standard --fix && tap --reporter=spec test/*.js",
        "build": "tsc",
        "coverage": "tap test/*.js --coverage",
        "coveralls": "tap --cov --coverage-report=lcov test/*.js && cat coverage/lcov.info | coveralls",
        "standard-fix": "standard --fix"
    },
    "files": [
        "lib/**/*"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/amindia/invoice-number.git"
    },
    "keywords": [
        "invoice",
        "number",
        "incrementer",
        "alphnumeric"
    ],
    "author": {
        "name": "KBala",
        "email": "kbala@adaptivemedia.in"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/amindia/invoice-number/issues"
    },
    "homepage": "https://github.com/amindia/invoice-number#readme",
    "devDependencies": {
        "coveralls": "^3.0.2",
        "standard": "^12.0.1",
        "tap": "^14.2.4",
        "typescript": "^3.5.2"
    }
}
